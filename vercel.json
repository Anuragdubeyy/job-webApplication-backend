{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/api/auth(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/api/employers(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/api/users(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }
